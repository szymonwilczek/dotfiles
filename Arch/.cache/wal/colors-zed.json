{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12141490",
                "editor.background": "#12141490",
                "editor.foreground": "#d5dede",
                "text": "#d5dede",
                "text.muted": "#d5dede70",
                "text.ignored": "#d5dede40",
                "text.placeholder": "#d5dede50",
                "ignored": "#d5dede30",
                "element.hover": "#d5dede30",
                "ghost_element.hover": "#9fb0b130",
                "ghost_element.selected": "#9fb0b130",
                "ghost_element.active": "#9fb0b160",
                "border": "#121414",
                "editor.highlighted_line.background": "#9fb0b110",
                "editor.active_line.background": "#9fb0b110",
                "panel.background": "#12141490",
                "title_bar.background": "#12141490",
                "title_bar.inactive_background": "#12141490",
                "status_bar.background": "#12141490",
                "drop_target.background": "#12141490",
                "elevated_surface.background": "#121414",
                "toolbar.background": "#12141490",
                "tab_bar.background": "#12141490",
                "tab.inactive_background": "#12141490",
                "tab.active_background": "#9fb0b130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#121414",
                "scrollbar.thumb.background": "#9fb0b1",
                "editor.gutter.background": "#12141490",
                "terminal.background": "#12141410",
                "terminal.foreground": "#d5dede",
                "terminal.dim_foreground": "#d5dede",
                "terminal.bright_foreground": "#d5dede",
                "terminal.ansi.black": "#121414",
                "terminal.ansi.red": "#9fb0b1",
                "terminal.ansi.green": "#c7c7dc",
                "terminal.ansi.yellow": "#8ae2f8",
                "terminal.ansi.blue": "#bedbdc",
                "terminal.ansi.magenta": "#8af5fa",
                "terminal.ansi.cyan": "#93fcd5",
                "terminal.ansi.white": "#bafd89",
                "terminal.ansi.bright_black": "#acb4b4",
                "terminal.ansi.bright_red": "#abbabb",
                "terminal.ansi.bright_green": "#cecee0",
                "terminal.ansi.bright_yellow": "#9ce5f9",
                "terminal.ansi.bright_blue": "#c7dfe0",
                "terminal.ansi.bright_magenta": "#9df6fa",
                "terminal.ansi.bright_cyan": "#a4fcda",
                "terminal.ansi.bright_white": "#c3fe9b",
                "modified": "#9fb0b1",
                "syntax": {
                    "attribute": {
                        "color": "#9fb0b1"
                    },
                    "boolean": {
                      "color": "#c7c7dc"
                    },
                    "comment": {
                      "color": "#acb4b4"
                    },
                    "comment.doc": {
                      "color": "#acb4b4"
                    },
                    "constant": {
                      "color": "#cecee0"
                    },
                    "function": {
                      "color": "#a4fcda"
                    },
                    "keyword": {
                      "color": "#bedbdc"
                    },
                    "number": {
                      "color": "#8af5fa"
                    },
                    "operator": {
                      "color": "#bedbdc"
                    },
                    "string": {
                      "color": "#9fb0b1"
                    },
                    "variable": {
                      "color": "#c7c7dc"
                    }
                },
                "players": [
                  {
                    "cursor": "#9fb0b1",
                    "background": "#121414",
                    "selection": "#9fb0b130"
                  }
                ]
            }
        }
    ]
}
